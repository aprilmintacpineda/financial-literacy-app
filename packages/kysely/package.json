{
  "name": "@packages/kysely",
  "version": "0.0.0",
  "description": "Kysely to manage all database stuff for my financial literacy app",
  "private": true,
  "scripts": {
    "ts-check": "tsc --noEmit",
    "eslint": "eslint . --fix",
    "prettier": "prettier . --write",
    "lint": "pnpm run prettier && pnpm run eslint",
    "migrate:all": "kysely migrate:latest",
    "migrate:up": "kysely migrate:up",
    "migrate:down": "kysely migrate:down",
    "migrate:make": "kysely migrate:make",
    "migrate:rollback": "kysely migrate:rollback",
    "seed:run": "kysely seed:run",
    "seed:make": "kysely seed:make",
    "codegen": "kysely-codegen --out-file models.ts",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "kysely": "^0.28.2",
    "kysely-codegen": "^0.18.5",
    "kysely-ctl": "^0.13.1",
    "mysql2": "^3.14.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.6.0",
    "@types/node": "^24.0.7",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.2.0",
    "lint-staged": "^16.1.2",
    "typescript-eslint": "^8.35.0",
    "zod": "^3.25.67"
  },
  "exports": {
    ".": [
      "./database.ts"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,html,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
